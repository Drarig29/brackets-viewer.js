{
    "name": "brackets-viewer",
    "version": "1.3.3",
    "description": "A simple library to display tournament brackets (round-robin, single elimination, double elimination)",
    "files": [
        "dist/"
    ],
    "scripts": {
        "db": "npx json-server --watch demo/db.json",
        "build": "npx webpack --mode production",
