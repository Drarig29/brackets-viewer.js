        "prepublishOnly": "npm run build",
        "lint": "eslint **/*.{js,ts} --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Drarig29/brackets-viewer.js.git"
    },
    "keywords": [
        "javascript",
        "tournament",
        "brackets",
        "viewer",
        "display"
    ],
